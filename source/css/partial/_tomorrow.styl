.highlight
  overflow: auto
  background-color: $highlight-background
  color: #CCCCCC
  .gutter
    color: #666666
  figcaption
    margin-bottom: 1em
    color: #999999
    font-size: .85em
    a
      float: right
  .comment
  .title
    color: $highlight-comment
  .variable
  .attribute
  .tag
  .regexp
  .ruby .constant
  .xml .tag .title
  .xml .pi
  .xml .doctype
  .html .doctype
  .css .id
  .css .class
  .css .pseudo
    color: $highlight-red
  .number
  .preprocessor
  .built_in
  .literal
  .params
  .constant
    color: $highlight-orange
  .class
  .ruby .class .title
  .css .rules .attribute
    color: $highlight-green
  .string
  .value
  .inheritance
  .header
  .ruby .symbol
  .xml .cdata
    color: $highlight-green
  .css .hexcolor
    color: $highlight-aqua
  .function
  .python .decorator
  .python .title
  .ruby .function .title
  .ruby .title .keyword
  .perl .sub
  .javascript .title
  .coffeescript .title
    color: $highlight-blue
  .keyword
  .javascript .function
    color: $highlight-purple